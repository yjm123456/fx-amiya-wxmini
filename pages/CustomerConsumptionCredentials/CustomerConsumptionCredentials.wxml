<view class="list">
    <block wx:for="{{consumptionCredentialsList}}">
        <view class="item" data-id="{{item.id}}" data-url="/pages/UpdateCustomerConsumptionCredential/UpdateCustomerConsumptionCredential" bindtap="toDetail">
            <view class="content">
                <view class="time">消费时间:{{item.CreateDate1}}</view>
                <view class="name">审核状态:{{item.checkStateText}}</view>
            </view>
            <view class="picture">
                <image src="{{item.payVoucherPicture1}}" mode="aspectFill"></image>
            </view>
        </view>
    </block>
</view>
<view wx:if="{{!consumptionCredentialsList.length}}" class="submit2" catchtap="to" data-url="/pages/AddConsumptionCredential/AddConsumptionCredential">添加消费凭证</view>
<view wx:else class="submit" catchtap="to" data-url="/pages/AddConsumptionCredential/AddConsumptionCredential">添加消费凭证</view>
