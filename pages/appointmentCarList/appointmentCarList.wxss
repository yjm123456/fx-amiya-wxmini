/* pages/appointmentCarList/appointmentCarList.wxss */
page{
    background-color: #000;
}
.container{
    padding: 25rpx;
    position: relative;
}
.item{
    color: #fff;
    background-color: #262626;
    padding: 20rpx;
    border-radius: 20rpx;
    font-size: 28rpx;
    margin-top: 20rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
}
.car_image{
    width:150rpx;
    height: 150rpx;
    border-radius: 20rpx;
    overflow: hidden;
}
.car_image>image{
    width: 100%;
    height: 100%;
}
.appointment_info{
    display: flex;
    flex-direction: column;
}