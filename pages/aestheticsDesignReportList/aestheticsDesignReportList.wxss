/* pages/CustomerConsumptionCredentials/CustomerConsumptionCredentials.wxss */
page{
    background-color: #f1f1f1;
    width: 100%;
    height: 100%;
}
.content{
    width: 750rpx;
    display: flex;
    flex-direction: column;
    color: #000;
    padding-bottom: 200rpx;
}
.item{
    background-color: #fff;
    padding: 15rpx;
    margin-top: 20rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.cell_left{
    font-size: 28rpx;
    display: flex;
    flex-direction: column;
    width: 50vw;
    justify-content: space-between;
}
.cell_right{
    display: flex;
    flex-direction: row;
    width: 40vw;
    justify-content: space-around;
}
.cell_right>image{
    width: 18vw;
    height: 19vw;
    border-radius: 20rpx;
}
.submit2{
    margin-top: 0;
}
.submit{
    color: #fff;
    position: fixed;
    bottom: 50rpx;
    background-color: #8878f5;
    width: 600rpx;
    margin: 0rpx 75rpx;
    height: 80rpx;
    border-radius: 20rpx;
    line-height: 80rpx;
    text-align: center;
}
