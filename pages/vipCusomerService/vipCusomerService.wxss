
page{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    height: 100%;
}
.top{
    margin-top: 40rpx;
}
.service{
    width: 550rpx;
    border-radius: 20rpx;
    overflow: hidden;
}
.service>image{
    width: 100%;
}
.vip{
    margin-top: 30rpx;
    font-size: 55rpx;
    display: flex;
    flex-direction: column;
    align-content: center;
    text-align: center;
    font-weight: 600;
    padding-bottom: 200rpx;
    color: #fc4e01;
}
.group{
    display: flex;
    width: 750rpx;
    justify-content: space-around;
}
.service2{
    width: 280rpx;
    height: 280rpx;
    border-radius: 20rpx;
    overflow: hidden;
    padding: 30rpx;
    background-image: url('https://ameiya.oss-cn-hangzhou.aliyuncs.com/8f40c3f1b33b4f7d833226b8c47f4ce4.png');
    background-size: 340rpx 340rpx;
    background-repeat: no-repeat;
    display: flex;
    justify-content: center;
    align-items: center;
}
.service2>image{
    width: 220rpx !important;
    height: 220rpx !important;
}

.warp{
    display: flex;
    flex-direction: column;
    align-content: center;
}
.center{
    margin-top: 20rpx;
    text-align: center;
    color: #8878f5;
    font-size: 35rpx;
    font-weight: 600;
}
.imageWarp{
    display: flex;
    flex-direction: column;
    align-items: center;
}
.jnWarp{
    display: flex;
}