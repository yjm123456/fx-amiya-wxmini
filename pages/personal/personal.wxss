.member_no{
    width: 145rpx;
    height: 155rpx;
    justify-self: right;
}
/* vip-box */
.vip_card_box {
    position: relative;
    font-size: 0;
}

.vip_card_box image {
    width: 100%;
    height: auto
}

.vip_card_box .up {
    position: absolute;
    z-index: 1;
    bottom: 40rpx;
    left: 40rpx;
    color: #fff;
    font-size: 30rpx;
}

.cover_container {
    margin-top: 10rpx;
    padding: 0 20rpx;
    background: #000000;
    padding-bottom: 20rpx;
    border-radius: 20rpx;
    overflow: hidden;
}
.card{
    width: 620rpx;
    z-index: 99;
    margin: 0 auto;
    position: relative;
    bottom: -20rpx;
}
.card image{
    width: 100%;
}
.bottom{
    height: 100rpx;
    background-color: #000;
}
