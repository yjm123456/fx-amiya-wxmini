page{
    background-color: #000000;
}
.personal_container{
    background-color: #000000;
    margin-bottom: 0;
    height: 100%;
}
.con_btn{
    width: 200rpx;
    background-color: #fff;
    border: 0rpx #fff;
    width: 100%;
}
.con_btn::after{
    border: 0;
}
.user_section{
    background-color: #313131;
    border-radius: 0 0 50% 50% / 0 0 10% 10%;
    position: relative;
    overflow: hidden;
}

.user_info_box {
    height: 220rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    width: 100%;
    padding-left: 30rpx;
    align-items: center;
    position: relative;
}
.bottom_image{
    width: 100%;
    position: absolute;
    left: 0;
    bottom: 0;
}
.bottom_image image{
    width: 100%;
    height: 100%;
}
.user_info_box .portrait {
    margin-right: 30rpx;
    width: 120rpx;
    height: 120rpx;
    border: 5rpx solid #939393;
    border-radius: 50%;
}

.user_info_box .info_box {
    height: 110rpx;
    width: 355rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    overflow: hidden;
}
.info_box_top{
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: flex-start;
}
.user_info_box .info_box .username {
   max-width: 80%;
   overflow: hidden;
   text-overflow: ellipsis;
    font-size: 28rpx;
    color: #fff;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    word-break: break-all;

}

.user_info_box .info_box .info_box_bottom {
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
}

.leave {
    background-image:-webkit-linear-gradient(right,#efcd71,#fff9c8,#efcf77,#fff9c8,#e4c675,#fff9c8) !important; 
    -webkit-background-clip:text !important; 
    -webkit-text-fill-color:transparent !important;
    font-size: 28rpx;
    background-color: #000;
    padding: 5rpx 10rpx; 
    border-radius: 20rpx;
    margin-left: 20rpx;
}

.leave_img {
width: 200rpx;
height: 80rpx;
border-radius: 10rpx;
}
/* .balance{
    font-size: 28rpx;
    color: #fff;
} */
.balance{
    background-color: #000;
    color: #dec350;
    padding:0 10rpx;
    font-size: 28rpx;
    display: inline-block;
    width: max-content;
    border-radius: 20rpx;
}
.balance>text{
    background-image:-webkit-linear-gradient(right,#efcd71,#fff9c8,#efcf77,#fff9c8,#e4c675,#fff9c8) !important; 
    -webkit-background-clip:text !important; 
    -webkit-text-fill-color:transparent !important;
}
.balance_amount{
    margin-left: 10rpx;
    color: #DEC350;
}
.bottom_left {
    height: 60rpx;
    line-height: 60rpx;
}
.navigation_list{
    padding-left: 30rpx;
    padding-right: 30rpx;
    padding-top: 10rpx;
    padding-bottom: 30rpx;
    height: 100rpx;
    line-height: 100rpx;
    overflow: hidden;
    display: flex;
    justify-content: space-between;
    /* background-color: #313131; */
    color: #DEC350;
  }
  .grid_navigation{
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
    line-height: 100%;
    border: #f4f4f4;
    overflow: hidden;
  }
  .grid_item{
      background-color: #3C3C3C;
      width: 220rpx;
      height: 100%;
      border-radius: 20rpx;
      text-align: center;
  }
  .grid_item>text{
    background-image:-webkit-linear-gradient(right,#efcd71,#fff9c8,#efcf77,#fff9c8,#e4c675,#fff9c8) !important; 
    -webkit-background-clip:text !important; 
    -webkit-text-fill-color:transparent !important;
    font-weight: 600;
  }
.bottom_right {
    width: 180rpx;
    height: 60rpx;
    width: 50%;
}

