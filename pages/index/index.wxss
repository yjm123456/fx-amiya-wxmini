.notice {
    background-color: #262626;
    border-radius: 20rpx;
    text-align: center;
    height: 65rpx;
    margin-top: 20rpx;
    /* height: 65rpx; */
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.notice>image {}

.notice_bar {
    height: 100% !important;
}

.van-notice-bar__content {
    font-weight: 600;
    /* color: #DEC350; */
    background-image: -webkit-linear-gradient(right, #efcd71, #fff9c8, #efcf77, #fff9c8, #e4c675, #fff9c8) !important;
    -webkit-background-clip: text !important;
    -webkit-text-fill-color: transparent !important;
}

.van-icon {
    font-weight: 600;
    /* color: #DEC350; */
    background-image: -webkit-linear-gradient(right, #efcd71, #fff9c8, #efcf77, #fff9c8, #e4c675, #fff9c8) !important;
    -webkit-background-clip: text !important;
    -webkit-text-fill-color: transparent !important;
}

.navigation_list {
    padding-top: 30rpx;
    padding-bottom: 30rpx;
    height: 100rpx;
    line-height: 100rpx;
    overflow: hidden;
    display: flex;
    justify-content: space-between;
}

.grid_navigation {
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
    line-height: 100%;
    border: #f4f4f4;
    overflow: hidden;

}

.grid_item {
    background-color: #262626;
    width: 220rpx;
    height: 100%;
    border-radius: 20rpx;
    text-align: center;
}

.activity {
    margin-top: 20rpx;
    display: flex;
    overflow: hidden;
    justify-content: space-between;
    height: 360rpx;
}

.activity_item {
    width: 100%;
    border-radius: 20rpx;
    background-color: #fff;
    overflow: hidden;
}

.activity_item image {
    width: 100%;
    height: 100%;
}

.banner2 {
    margin-top: 20rpx;
    width: 690rpx;
    height: 320rpx;
}

.item {
    box-sizing: border-box;
    width: 300rpx !important;
    height: 310rpx !important;
    border-radius: 30rpx;
    padding: 10rpx;
}

.item>image {
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
}


.navigation .grid {
    height: 100rpx;
    line-height: 100rpx;
}

.title_con,
.consulting_service {
    padding: 20rpx 30rpx;
    box-sizing: border-box;
    background-color: #fff;
}

.title_con {
    color: #ff1554;
    text-align: center;
}

.h2 {
    font-size: 36rpx;
    font-weight: bold;
}

.h5 {
    font-size: 28rpx;
    margin-left: 30rpx;
}

.consulting_service {
    display: flex;
    align-items: center;
    margin-top: 20rpx;
}

.img {
    width: 100rpx;
    height: 100rpx;
    border-radius: 10rpx;
}

.consulting_center {
    flex: 1;
    margin-left: 20rpx;
}

.h3 {
    font-size: 30rpx;
    font-weight: bold;
}

.h4 {
    font-size: 24rpx;
    color: #818181;
    margin-top: 20rpx;
}

.service {
    font-size: 24rpx;
    padding: 5rpx 8rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
    border: 1rpx solid #ffa001;
    color: #ffa001;
    text-align: center;
}


.diary_con {
    padding: 30rpx 30rpx 15rpx;
    box-sizing: border-box;
    background-color: #fff;
    margin: 20rpx 0;
}

.diary_like_con {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.diary_img {
