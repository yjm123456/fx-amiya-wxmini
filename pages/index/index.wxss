page {
    height: 100%;
    background-color: #f1f1f1;
}

.container {
    padding: 30rpx;
    background-color: #f1f1f1;
}


/* 轮播图 */
.banner_container {}

.banner {
    background-color: transparent;
    height: 400rpx;
    
    overflow: hidden;
    transform: translateY(0);
}


.banner .slide {
    width: 100%;
    height: 100%;
}

/* =================================================== */
/* 变美计划/爆品分类/热卖大牌 */
.warp{
    background-color: #fff;
    padding: 20rpx;
    border-radius: 20rpx;
    margin-top: 20rpx;
}
.title {
    margin: 20rpx 0;
}

.title>text {
    font-size: 28rpx;
    font-weight: 600;
}

.mbanner {
    margin-top: 20rpx;
    width: 670rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}

.mbanner-left {
    display: flex;
    flex-direction: row;
    width: 320rpx;
    justify-content: space-between
}

.mbanner-right {
    display: flex;
    flex-direction: row;
    width: 320rpx;
    justify-content: space-between;
}

.mbanner-item {
    border-radius: 20rpx;
    width: 130rpx;
    font-size: 22rpx;
    color: #000;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}



