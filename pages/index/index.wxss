.mbanner-img {
    width: 80rpx;
    height: 80rpx;
}
.mbanner-img-warp{
    width: 110rpx;
    height: 110rpx;
    
    border-radius: 20rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.mbanner-img-ca{
    width: 100%;
    z-index:2;
}
.mbanner-text{
    margin-top: 20rpx;
    font-size: 26rpx;
}
.brand{
    width: 130rpx;
    height: 130rpx;
    box-shadow: 1rpx 3rpx 10rpx #e2e2e2;
    border-radius: 20rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.brand>image{
    width: 110rpx;
    height: 110rpx;
}
/* ================================================================ */

/* 热卖商品 */
.hot_product {
    display: flex;
    flex-direction: column;
    margin-top: 20rpx;
}

.product {
    display: flex;
    margin-bottom: 30rpx;
}
.product>.img_warp{
   
    width: 180rpx;
    height: 180rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
}
.img_warp>image {
    width: 160rpx;
    height: 160rpx;
    border-radius: 20rpx;
    flex: 2;
}

.product_desc {
    flex: 6;
    display: flex;
    flex-direction: column;
    font-size: 28rpx;
    justify-content: space-between;
    margin-left: 20rpx;
}

.product_desc>.name {
    font-size: 28rpx;
    font-weight: 600;
    overflow: hidden; 
    text-overflow: ellipsis;
    display: -webkit-box; 
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2; 
}

.product_desc>.price {
    font-size: 32rpx;
}

.price>.min_price {
    font-weight: 600;
    color: #fc4e01;
}

.price>.origin_price {
    margin-left: 10rpx;
    color: #949494;
    text-decoration: line-through;
}
.p_tag{
    color: #fff;
    font-size: 25rpx;
    background-color: #dcd0ff;
    padding: 3rpx 6rpx;
    border-radius: 15rpx;
    width: fit-content;
}
/* ================================================================= */

.discount {
    margin-top: 20rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.discount-item {
    width: 340rpx;
    border-radius: 20rpx;
    overflow: hidden;
    position: relative;
}

.discount-item>image {
    width: 100%;
}

.join {
    position: absolute;
    width: 120rpx !important;
    z-index: 5;
    bottom: 70rpx;
    left: 230rpx;
}

