/* 轮播图 */
.banner {
    background-color: #fff;
    height: 400rpx;
    border-radius: 20rpx;
    overflow: hidden;
    /* border: 2rpx solid transparent; */
    background-clip: padding-box, border-box;
    background-origin: padding-box, border-box;
    background-image: linear-gradient(to right, #efcd71, #fff9c8, #efcf77, #fff9c8, #e4c675, #fff9c8), linear-gradient(90deg, #efcd71, #fff9c8, #efcf77, #fff9c8, #e4c675, #fff9c8);
}

.banner .slide {
    width: 100%;
    height: 100%;
}
.mbanner{
    margin-top: 20rpx;
    box-sizing: border-box;
    width: 710;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.mbanner-left{
    display: flex;
    flex-direction: row;
    width: 320rpx;
    justify-content: space-between
}
.mbanner-right{
    display: flex;
    flex-direction: row;
    width: 320rpx;
    justify-content: space-between;
}
.mbanner-item{
    border-radius: 20rpx;
    width: 150rpx;
    height: 150rpx;
    font-size: 28rpx;
    background-color: #262626;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.mbanner-item>view{
    padding-bottom: 10rpx;
}
.mbanner-text{
    background-image: -webkit-linear-gradient(right, #e7cdc4, #fffaf3,#eacdbe,#fbeee5,#e4cfbb);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
}
.mbanner-img{
    width: 80%;
    height: 80%;
}
.discount{
    margin-top: 20rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.discount-item{
    width: 340rpx;
    border-radius: 20rpx;
    overflow: hidden;
    position: relative;
}
.discount-item>image{
    width: 100%;
}
.join{
    position: absolute;
    width: 120rpx !important;
    z-index: 5;
    bottom: 70rpx;
    left: 230rpx;
}
