    border-radius: 10rpx;
    margin: 10rpx 0;
    width: 100%;
    height: 400rpx;
}

.diary_title {
    font-size: 28rpx;
    font-weight: bold;
}

.diary_right {
    font-weight: bold;
    font-size: 30rpx;
